    "drilldown": {
        "roles": [
            "categoryGroup"
        ]
    },
    "supportsHighlight": true,
    "dataViewMappings": [
        {
            "conditions": [
                {
                    "category": {
                        "max": 1
                    },
                    "measure": {
                        "max": 1
                    },
                    "categoryGroup": {
                        "max": 1
                    },
                    "xCategoryParent": {
                        "max": 1
                    },
                    "yCategoryParent": {
                        "max": 1
                    },
                    "tooltipData": {
                        "max": 4
                    },
                    "categorySize": {
                        "max": 1
                    },
                    "categoryColor": {
                        "max": 1
                    }
                }
            ],
            "categorical": {
                "categories": {
                    "select": [
                        {
                            "for": {
                                "in": "category"
                            },
                            "dataReductionAlgorithm": {
                                "top": {
                                    "count": 50000
                                }
                            }
                        },
                        {
                            "for": {
                                "in": "categoryGroup"
                            }
                        },
                        {
                            "for": {
                                "in": "tooltipData"
                            }
                        },
                        {
                            "for": {
                                "in": "xCategoryParent"
                            }
                        },
                        {
                            "for": {
                                "in": "yCategoryParent"
                            }
                        }
                    ]
                },
                "values": {
                    "group": {
                        "by": "categoryColor",
                        "select": [
                            {
                                "bind": {
                                    "to": "measure"
                                }
                            },
                            {
                                "bind": {
                                    "to": "categorySize"
                                }
                            }
                        ]
                    },
                    "dataReductionAlgorithm": {
                        "top": {
                            "count": 50000
                        }
                    }
                }
            }
        }
    ],
